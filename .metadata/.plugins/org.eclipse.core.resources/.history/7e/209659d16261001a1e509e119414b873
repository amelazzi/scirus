		/**
 * @generated
 */
	public static boolean isSemanticLeafVisualID(int visualID) {
		switch (visualID) {
						case entities.diagram.edit.parts.NetworkEditPart.VISUAL_ID:
				return false;
									case entities.diagram.edit.parts.DataBaseEditPart.VISUAL_ID:case entities.diagram.edit.parts.CapteurEditPart.VISUAL_ID:case entities.diagram.edit.parts.GateWayEditPart.VISUAL_ID:case entities.diagram.edit.parts.CloudEditPart.VISUAL_ID:
				return true;
									default:
				break;
		}
		return false;
	}

